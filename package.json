  },
  "dependencies": {
    "next": "^12.1.0",
    "react": "^18.0.0-rc.2",
    "react-dom": "^18.0.0-rc.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
}
