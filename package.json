{
  "name": "dryadsoft-privacy-policy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "gh-pages": "gh-pages -t -d out",
    "remove:cache": "rm -rf node_modules/.cache",
    "deploy:dev": "npm run remove:cache && npm run build && npm run export && touch out/.nojekyll",
